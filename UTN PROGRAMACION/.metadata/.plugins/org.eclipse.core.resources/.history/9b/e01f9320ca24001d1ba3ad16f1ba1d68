#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int promedio;
	int numeroIngresado;
	int total;
	for(int i = 0;i < 5; i++){

		printf("ingrese un numero: ");
		scanf("%d", &numeroIngresado);

		total = total + numeroIngresado;
	}
	promedio = total / 5;

	printf("El promedio es: %d", &promedio);
	return 0;
}
